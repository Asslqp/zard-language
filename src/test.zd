main() {
    int a;
    input a;
    print(a);

    while (a < 8) {
        if (a == 3) {
            print("A é igual a 3");
        } else if (a == 5) {
            print("A é igual a 5");
        } else if (a == 7) {
            print("A é igual a 7");
        } else {
            print("A é diferente de 3, 5 e 7");
        }
        a++;
        print(a);
    }
    if (a == 9) {
        print("A chegou a 10");
    } else
     {
        print("A não é 10, é " + a);
    }
}
